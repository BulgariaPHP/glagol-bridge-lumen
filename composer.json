{
    "name": "bulgaria-php/glagol-php-dic-laravel",
    "description": "Dependency inverted container for accessing the Laravel Container",
    "type": "library",
    "require": {
        "psr/container": "dev-master",
        "illuminate/container": "^5.3",
        "bulgaria-php/glagol-php-dic": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "5.6.*"
    },
    "license": "gnu-gpl3",
    "authors": [
        {
            "name": "Yoan-Alexander Grigorov",
            "email": "joan.grigorov@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {"Glagol\\Binding\\Laravel\\": "src/"}
    },
    "autoload-dev": {
        "psr-4": {"GlagolTest\\Binding\\Laravel\\": "test/"}
    }
}
